#include "3-calc.h"

/**
 * op_add - sum of two number
 * @a: int value
 * @b: int value
 * Return: sum of a and b
 */

int op_add(int a, int b)
{
	return (a + b);
}

/**
 * op_add - substract of two number
 * @a: int value
 * @b: int value
 * Return: substract of a and b
 */

int op_sub(int a, int b)
{
	return (a - b);
}

/**
 * op_mul - multiplication of two number
 * @a: int value
 * @b: int value
 * Return: multiplication of a and b
 */

int op_mul(int a, int b)
{
	return (a * b);
}

/**
 * op_div - division of two number
 * @a: int value
 * @b: int value
 * Return: division of a and b
 */

int op_div(int a, int b)
{
	return (a / b);
}

/**
 * op_mod - remainder of two number
 * @a: int value
 * @b: int value
 * Return: modulos of a and b
 */

int op_mod(int a, int b)
{
	return (a % b);
}
